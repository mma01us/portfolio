	<script>

        function getCurrentPortfolioId() {
            let fullPath = window.location.pathname.split('/');
            if(fullPath[2].length > 8) {
                return fullPath[2].slice(0,8);
            } else {
                return null;
            }
        }

        function navigateBack() {
		    window.history.back();
        }

        function navigeteToProject(projectId, projectName) {
            let currentPortfolioId = getCurrentPortfolioId();
            var location = "/project/";
            if(currentPortfolioId) {
                location += currentPortfolioId;
            }
            location += projectId;
            location += ("/" + projectName);

			window.location.href = location;
		}

        function navigationStickyHeader() {
			// sticky header
			var startPoint = document.getElementsByClassName('sticky-header'),
				navigationItems = document.getElementsByClassName('navigation-steps');
			for (var i = 0; i < startPoint.length; i++) {
				var target = startPoint[i].querySelector('.navigation-steps'),
					lastStep = 'section-'+startPoint[i].getAttribute('data-end'),
					startY = startPoint[i].getBoundingClientRect().top,  // stick starts here
					endY = startPoint[i].getAttribute('data-end') ? document.getElementById(lastStep).getBoundingClientRect().top + document.getElementById(lastStep).getBoundingClientRect().height - 70 : 0, // stick ends here
					steps = navigationItems[i].getElementsByClassName('nav-step');
				if (startY >= 0) {
					target.classList.remove('sticked');
				} else if (endY <= 0) {
					target.classList.add('sticked-hidden');
				} else if (startY <= 0) {
					target.classList.remove('sticked-hidden');
					target.classList.add('sticked');
					for (var k = 0; k < steps.length; k++) {
						var currentStickyStep = target.getElementsByClassName('nav-step')[k],
							stepAnchor = document.getElementById(currentStickyStep.querySelector('.step-link').getAttribute('href').split('#')[1]),
							stepY = stepAnchor ? stepAnchor.getBoundingClientRect().y : window.innerHeight + 1;
						( stepY <= 70 ) ? currentStickyStep.classList.add('checked') : currentStickyStep.classList.remove('checked');
					}
				}
			}
        }

        document.addEventListener('scroll', navigationStickyHeader);

        document.addEventListener("DOMContentLoaded", () => {
            scrollToItem('.step-link', 300, 60);
        });

        function scrollToItem (selector, duration, fps) {
            let anchors = [].slice.call(document.querySelectorAll(selector));
            for (let anchor of anchors) {
                anchor.addEventListener('click', scrollAnimation);
            }
            function scrollAnimation(e) {
                e.preventDefault();
                let target = document.querySelector(this.getAttribute('href'));
                let element = target, targetOffset = 0;
                while (element && element !== document.body) {
                    targetOffset += element.offsetTop;
                    element = element.offsetParent;
                }
                let currentOffset = window.pageYOffset || document.body.scrollTop;
                let difference = Math.abs(currentOffset - targetOffset);
                if (!difference) return;
                let frames = parseInt(duration / 1000 * fps);
                let tick = duration / frames;
                let perFrame = difference / frames;
                let direction = (targetOffset > currentOffset) ? 1 : -1;
                let timer = setInterval(() => {
                    currentOffset = window.pageYOffset || document.body.scrollTop;
                    difference = Math.abs(currentOffset - targetOffset + 69);
                    if (difference < perFrame) {
                        scrollBy(0, difference * direction);
                        clearInterval(timer);
                        return;
                    }
                    scrollBy(0, perFrame * direction);
                    if (window.pageYOffset + window.innerHeight === document.body.scrollHeight) clearInterval(timer);
                }, tick);
            }
        }

        var galleries = document.getElementsByClassName("carousel-gallery");
        if(galleries.length > 0) {
			Array.prototype.forEach.call(galleries, function(elem) {
			    let imageNumber = 4;
			    if(elem.classList.contains('desktop-mockup')) {
			        imageNumber = 2;
				}
				new Glide(elem, {
					type: 'carousel',
					startAt: 0,
					perView: imageNumber,
					breakpoints: {
					    576: {perView: imageNumber == 2 ? 1 : 2},
						768: {perView: imageNumber == 2 ? imageNumber : 3},
						1200: {perView: imageNumber}
					},
					focusAt: 'center',
					gap: 30
				}).mount();
			});
        }

        function scrollPageToTop() {
            let isSmoothScrollSupported = 'scrollBehavior' in document.documentElement.style;
            if (isSmoothScrollSupported) {
                window.scroll({
                    top: 0,
                    left: 0,
                    behavior: 'smooth'
                });
            } else {
                smoothScroll(0, -1);
            }
        }

        function smoothScroll(targetOffset, direction) {
            let currentOffset = window.pageYOffset || document.body.scrollTop;
            let difference = Math.abs(currentOffset - targetOffset);
            if (!difference) return;
            let frames = parseInt(400 / 1000 * 60);
            let tick = 400 / frames;
            let perFrame = difference / frames;
            let timer = setInterval(() => {
                if (direction > 0) {
                    if (window.pageYOffset >= targetOffset) {
                        scrollBy(0, (targetOffset - difference) * direction);
                        clearInterval(timer);
                        return;
                    }
                } else {
                    if (window.pageYOffset <= 0) {
                        scrollBy(0, difference * direction);
                        clearInterval(timer);
                        return;
                    }
				}
                scrollBy(0, perFrame * direction);
                if (window.pageYOffset + window.innerHeight === document.body.scrollHeight) clearInterval(timer);
            }, tick);
        }

        function scrollDistance() {
            if (window.pageYOffset > 1200) {
                if(document.querySelector('.scroll-to-top').classList.contains('visible')) return;
                document.querySelector('.scroll-to-top').classList.add("visible");
			} else {
                document.querySelector('.scroll-to-top').classList.remove("visible");
			}
        }
        document.addEventListener('scroll', scrollDistance);

        function bindEvent(e) {
            if(e.origin !== 'https://uxstudioteam.com') return;
            try {
                eval('(' + decodeURI(e.data) + ')();');
            } catch(e) {}
        }
        window.addEventListener('message', bindEvent, false);

	</script>

